<h1>論文管理ツール</h1>
<h3>論文内容 詳細</h3>
<div class="paper">
  <p><%= @paper.user.email %></p>
  <p><%= @paper.name %></p>
  <p><%= @paper.title %></p>
  <p><%= @paper.author %></p>
  <p><%= @paper.year %></p>
  <p><%= @paper.journal %></p>
  <p><%= @paper.subject %></p>
  <p><%= @paper.purpose %></p>
  <p><%= @paper.method %></p>
  <p><%= @paper.result %></p>
</div>

<%= link_to "Tweet一覧に戻る", papers_path %>